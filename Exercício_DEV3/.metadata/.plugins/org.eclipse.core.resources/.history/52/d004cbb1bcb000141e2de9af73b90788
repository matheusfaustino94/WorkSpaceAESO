package br.aeso.aula03.tarefinha02;

public class Chassi {
	
	private String Numero;
	private int Ano;
	
	public String getNumero() {
		return Numero;
	}
	public void setNumero(String numero) {
		Numero = numero;
	}
	public int getAno() {
		return Ano;
	}
	public void setAno(int ano) {
		Ano = ano;
	}
	
	

}
